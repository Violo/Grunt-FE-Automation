<!doctype HTML>
<html>
  <head>
    <title>Icons Preview!</title>
    <script>
      {{{loaderText}}}
      grunticon(["icons.data.svg.css", "icons.data.png.css", "icons.fallback.css"]);
    </script>
  <noscript><link href="icons.fallback.css" rel="stylesheet"></noscript>
  </head>
  <body>
    {{#each icons}}
      {{#with this}}
      <pre><code>{{prefix}}{{name}}:</code></pre><div class="{{prefixClass}}{{name}}" style="width: 32px; height: 32px; background-size:32px 32px;" ></div><hr/>
      {{/with}}
    {{/each}}
</body>
</html>